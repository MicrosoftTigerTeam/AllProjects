{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "chat-app-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/chatApp/ChatAppWebPart.js",
          "manifest": "./src/webparts/chatApp/ChatAppWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js",
      "globalName": "jquery"
    }
  },
  "localizedResources": {
    "ChatAppWebPartStrings": "lib/webparts/chatApp/loc/{locale}.js"
  }
}
